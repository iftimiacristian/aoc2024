use common::ParseError;

pub fn parse_input(input: &str) -> Result<Vec<String>, ParseError> {
    Ok(input.lines().map(|s| s.to_string()).collect())
}

#[cfg(test)]
mod tests {
    use super::*;

    #[test]
    fn test_parse_input() {
        let input = "line1\nline2\nline3";
        let expected = vec!["line1".to_string(), "line2".to_string(), "line3".to_string()];
        assert_eq!(parse_input(input).unwrap(), expected);
    }
}

